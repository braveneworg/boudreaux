{
  "name": "boudreaux",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:cdn": "npm run build && npm run sync:cdn",
    "start": "next start",
    "lint": "eslint",
    "sync:cdn": "npx tsx scripts/sync-cdn.ts",
    "sync:cdn:no-build": "SKIP_BUILD=true npx tsx scripts/sync-cdn.ts",
    "docker:build:website": "docker build -t ghcr.io/braveneworg/boudreaux/website .",
    "docker:push:website": "docker push ghcr.io/braveneworg/boudreaux/website",
    "docker:build:nginx": "docker build -t ghcr.io/braveneworg/boudreaux/nginx .",
    "docker:push:nginx": "docker push ghcr.io/braveneworg/boudreaux/nginx",
    "docker:publish:website": "npm run docker:build:website && npm run docker:push:website",
    "docker:publish:nginx": "npm run docker:build:nginx && npm run docker:push:nginx"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.10.0",
    "mongodb": "^6.20.0",
    "mongoose": "^8.18.2",
    "next": "15.5.0",
    "next-auth": "^5.0.0-beta.29",
    "nodemailer": "^6.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "^3.658.1",
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/lib-storage": "^3.658.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/mime": "^4.0.0",
    "@types/node": "^20.19.14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "mime": "^4.0.4",
    "tailwindcss": "^4",
    "tsx": "^4.19.0",
    "typescript": "^5"
  }
}
